#!/bin/bash

###############################################################################
# CS173H

## tiling
./plot_horizontal_slice.py -b 32,-125.75 -u 42.5,-114.5 -s 0.05 -e 0 -d vs -a s -c cencal,cvms5,cs173h -o tiling_cencal_cvms5_cs173h.png

./plot_depth_profile.py -s 35.5,-119.125 -d vs,vp,density -g 2500 -e 5000 -c cs173h -v 20 -o depth_cs173h_d1.png

./plot_depth_profile.py -s 34.09986,-118.75974 -d vs,vp,density -g 2500 -e 5000 -c cs173h -v 20 -o depth_cs173h_d2.png

./plot_depth_profile.py -s 34.09259,-121.43373 -d vs,vp,density -g 2500 -e 5000 -c cs173h -v 20 -o depth_cs173h_d3.png

./plot_depth_profile.py -s 34.5,-118.67 -d vs,vp,density -g 2500 -e 5000 -c cs173h -v 20 -o depth_cs173h_d4.png

## outside..
./plot_depth_profile.py -s 31.266430,-116.485620 -d vs,vp,density -g 2500 -e 5000 -c cs173h -v 20 -o depth_cs173h_d5.png

./plot_depth_profile.py -s 38,-123 -d vs,vp,density -g 2500 -e 5000 -c cs173h -v 20 -o depth_cs173h_d6.png

# Z at 0 (z=0)
./plot_horizontal_slice.py -b 30,-130 -u 42,-113 -d vs -c cs173h -a s -s 0.05 -e 0 -o cs173h_hort_0.png

