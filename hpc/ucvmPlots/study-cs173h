./plot_horizontal_slice.py -b 30,-130 -u 42,-113 -d vs -c cs173h -a s -s 0.05 -e 1050 -o cs173h_hort_1050.png

# Z at 9975 (z=57)
./plot_horizontal_slice.py -b 30,-130 -u 42,-113 -d vs -c cs173h -a s -s 0.05 -e 9975 -o cs173h_hort_9975.png

# Z at 19950 (z=114)
./plot_horizontal_slice.py -b 30,-130 -u 42,-113 -d vs -c cs173h -a s -s 0.05 -e 19950 -o cs173h_hort_19950.png

# Z at 50400 (z=)287
./plot_horizontal_slice.py -b 30,-130 -u 42,-113 -d vs -c cs173h -a s -s 0.05 -e 50400 -o cs173h_hort_19950.png


